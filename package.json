{
  "name": "openapicmd",
  "description": "Command line tools for openapi-enabled APIs",
  "version": "0.1.7",
  "author": "Vilj7mi Kuosmanen <viljami@avoinsorsa.fi>",
  "bin": {
    "openapi": "./bin/run"
  },
  "bugs": "https://github.com/anttiviljami/openapicmd/issues",
  "dependencies": {
    "@oclif/command": "^1.5.11",
    "@oclif/config": "^1.12.9",
    "@oclif/plugin-help": "^2.1.6",
    "axios": "^0.18.0",
    "cli-ux": "^5.2.1",
    "get-port": "^5.0.0",
    "js-yaml": "^3.13.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-mount": "^4.0.0",
    "koa-proxy": "^1.0.0-alpha.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "openapi-backend": "^2.3.0",
    "swagger-editor-dist": "^3.6.24",
    "swagger-parser": "^6.0.5",
    "swagger-ui-dist": "^3.21.0",
    "swagger2openapi": "^5.2.5",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.2",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^10.12.29",
    "@types/swagger-ui-dist": "^3.0.0",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/anttiviljami/openapicmd",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "openapi",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "anttiviljami/openapicmd",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "watch-build": "tsc -w",
    "build": "tsc -b",
    "test": "jest -i",
    "lint": "tslint -p . -t stylish",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
